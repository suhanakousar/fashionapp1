{
  "buildCommand": "npm install --prefix vercel-deploy && npm run build --prefix vercel-deploy",
  "outputDirectory": "vercel-deploy/dist",
  "devCommand": "npm run dev --prefix vercel-deploy",
  "installCommand": "npm install --prefix vercel-deploy",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

