{
  "buildCommand": "cd vercel-deploy && npm install && npm run build",
  "outputDirectory": "vercel-deploy/dist",
  "devCommand": "cd vercel-deploy && npm run dev",
  "installCommand": "cd vercel-deploy && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/v1/(.*)",
      "destination": "/vercel-deploy/api/index.py"
    },
    {
      "source": "/health",
      "destination": "/vercel-deploy/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/vercel-deploy/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/vercel-deploy/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/vercel-deploy/index.html"
    }
  ]
}

